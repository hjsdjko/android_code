<?xml version="1.0" encoding="utf-8"?>
<com.design.appproject.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/add_box_style"
    android:fillViewport="true">
    <com.design.appproject.widget.MyLinearLayout
        style="@style/add_form_style"
        android:orientation="vertical">
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/wupinmingcheng_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="物品名称"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/wupinmingcheng_et"
            style="@style/add_item_input_style"
            android:hint="物品名称"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            android:id="@+id/tupian_ll"
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:id="@+id/tupian_tv"
                android:text="图片"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.MyImageView
                android:id="@+id/tupian_ifv"
                style="@style/add_item_img_style"
                android:src="@mipmap/upload"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="物品类型"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/wupinleixing_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择物品类型"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shuliang_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="数量"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shuliang_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="数量"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shiqudidian_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="拾取地点"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shiqudidian_et"
            style="@style/add_item_input_style"
            android:hint="拾取地点"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="拾取时间"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/shiqushijian_tv"
            android:text="请选择拾取时间"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shiquzhexingming_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="拾取者姓名"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shiquzhexingming_et"
            style="@style/add_item_input_style"
            android:hint="拾取者姓名"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shiquzhedianhua_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="拾取者电话"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shiquzhedianhua_et"
            style="@style/add_item_input_style"
            android:hint="拾取者电话"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/storeupnum_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="收藏数量"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/storeupnum_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="收藏数量"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="物品状态"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/wupinzhuangtai_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择物品状态"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        app:flexWrap="wrap"
        style="@style/add_item_editor_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="物品描述"
            style="@style/add_item_editor_label_style"/>
        <include
                android:id="@+id/wupinmiaoshu_rich_layout"
                layout="@layout/richeditor_layout"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFrameLayout
        style="@style/add_btn_box_style">
        <com.design.appproject.widget.MyButton
            android:id="@+id/submit_btn"
            android:text="提交"
            android:gravity="center"
        style="@style/add_btn_submit_style"/>
    </com.design.appproject.widget.MyFrameLayout>

    </com.design.appproject.widget.MyLinearLayout>
</com.design.appproject.widget.MyScrollView>
